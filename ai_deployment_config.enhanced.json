{
  "version": "2.0",
  "description": "ProjectMeats AI Deployment Orchestrator - Enhanced Configuration v2.0",
  
  "ai_features": {
    "intelligent_error_detection": true,
    "predictive_analysis": true,
    "autonomous_recovery": true,
    "performance_optimization": true,
    "learn_from_failures": true,
    "optimization_suggestions": true,
    "real_time_monitoring": true
  },
  
  "deployment": {
    "strategy": "ai_optimized",
    "max_retries": 3,
    "retry_delay": 5,
    "command_timeout": 300,
    "auto_recovery": true,
    "backup_on_failure": true,
    "enable_monitoring": true,
    "log_level": "INFO",
    "parallel_execution": false,
    "performance_optimization": true
  },
  
  "ssh": {
    "port": 22,
    "timeout": 30,
    "key_file": null,
    "use_agent": true,
    "strict_host_checking": false
  },
  
  "github": {
    "user": null,
    "token": null,
    "repository": "Vacilator/ProjectMeats",
    "branch": "main",
    "use_ssh": false
  },
  
  "server_profiles": {
    "production": {
      "hostname": "your-production-server.com",
      "username": "root",
      "domain": "yourdomain.com",
      "environment": "production",
      "use_password": false,
      "key_file": "~/.ssh/id_ed25519",
      "specs": {
        "cpu_cores": "4",
        "memory_gb": "8", 
        "storage_gb": "100",
        "ssd": true
      },
      "ai_optimizations": {
        "enable_parallel_tasks": true,
        "memory_optimization": true,
        "performance_tuning": true
      }
    },
    "staging": {
      "hostname": "staging.yourdomain.com",
      "username": "root", 
      "domain": "staging.yourdomain.com",
      "environment": "staging",
      "use_password": false,
      "key_file": "~/.ssh/id_ed25519",
      "specs": {
        "cpu_cores": "2",
        "memory_gb": "4",
        "storage_gb": "50",
        "ssd": true
      },
      "ai_optimizations": {
        "enable_parallel_tasks": false,
        "memory_optimization": false,
        "performance_tuning": false
      }
    },
    "development": {
      "hostname": "dev.yourdomain.com",
      "username": "ubuntu",
      "domain": "dev.yourdomain.com", 
      "environment": "development",
      "use_password": false,
      "key_file": "~/.ssh/id_ed25519",
      "specs": {
        "cpu_cores": "1",
        "memory_gb": "2",
        "storage_gb": "20",
        "ssd": false
      },
      "ai_optimizations": {
        "enable_parallel_tasks": false,
        "memory_optimization": true,
        "performance_tuning": false
      }
    }
  },
  
  "database": {
    "type": "postgresql",
    "host": "localhost",
    "port": 5432,
    "name": "projectmeats",
    "user": "projectmeats",
    "auto_create": true,
    "backup_enabled": true
  },
  
  "security": {
    "firewall_enabled": true,
    "ssl_enabled": true,
    "fail2ban_enabled": true,
    "auto_security_updates": true,
    "allowed_ports": [22, 80, 443],
    "ssl_provider": "letsencrypt"
  },
  
  "monitoring": {
    "enabled": true,
    "health_checks": true,
    "performance_monitoring": true,
    "error_tracking": true,
    "log_aggregation": true,
    "alerts": {
      "email": null,
      "webhook": null,
      "discord": null
    }
  },
  
  "backup": {
    "enabled": true,
    "frequency": "daily",
    "retention_days": 7,
    "compress": true,
    "offsite_backup": false
  },
  
  "performance": {
    "cache_enabled": true,
    "compression_enabled": true,
    "cdn_enabled": false,
    "database_optimization": true,
    "static_file_optimization": true
  },
  
  "logging": {
    "level": "INFO",
    "max_files": 10,
    "max_size": "50MB",
    "format": "detailed",
    "include_timestamps": true,
    "include_metrics": true
  },
  
  "ai_learning": {
    "collect_metrics": true,
    "learn_from_errors": true,
    "improve_predictions": true,
    "share_anonymous_stats": false
  },
  
  "_metadata": {
    "created": "2024-01-03T00:00:00Z",
    "last_modified": "2024-01-03T00:00:00Z",
    "version_history": [
      "1.0 - Initial configuration system",
      "2.0 - Enhanced AI features and consolidation"
    ]
  },
  
  "_instructions": {
    "setup": "1. Update server_profiles with your actual server details",
    "github": "2. Add GitHub username and token for private repository access",
    "security": "3. Configure SSL domain and security settings",
    "deploy": "4. Run: python3 ai_deployment_orchestrator.py --interactive",
    "help": "For detailed instructions, see DEPLOYMENT_MASTER_GUIDE.md"
  },
  
  "_examples": {
    "quick_start": "python3 ai_deployment_orchestrator.py --profile=production",
    "interactive": "python3 ai_deployment_orchestrator.py --interactive",
    "with_github": "python3 ai_deployment_orchestrator.py --server=myserver.com --domain=mydomain.com --github-user=USERNAME --github-token=TOKEN",
    "test_connection": "python3 ai_deployment_orchestrator.py --test-connection --server=myserver.com",
    "ai_recovery": "python3 ai_deployment_orchestrator.py --resume --deployment-id=abc123 --ai-recovery"
  }
}